package ex03;

import java.util.Scanner;

public class Sample {

	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		
		boolean run=true;
		
			System.out.println("----------------------------------------------------");
			System.out.println("1.번 주소입력 | 2.번 주소목록 | 3.번 주소 조회 | 4.번 프로그램종료");
			System.out.println("----------------------------------------------------");
			System.out.print("선택> ");
			
		while(run) {
			
			
			int menu=s.nextInt();
			
			switch(menu) {
				
				case 1:
					
					break;
				case 2:
					
					break;
				case 3:
					
					break;
				case 4:
					System.out.println("프로그램 종료");
					run=false;
			}
		}

	}

}
