package ex01;

public class Student {
	//필드 //Student라는 클래스가 가지는 속성 /객체를 생성함  
	String sno;
	String sname;
	String tel;
	
	
	
	//기본생성자
	Student() {
		
			
	}public void print(Student a) {
		System.out.println("--------------------------------------------");
		System.out.println("번호 : "+ a.sno);
		System.out.println("이름 : "+ a.sname);
		System.out.println("전화번호 : "+ a.tel);
		
	}
}
