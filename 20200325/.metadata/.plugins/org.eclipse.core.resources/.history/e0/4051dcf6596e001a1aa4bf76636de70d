package ex04;

public class Sample {

	public static void main(String[] args) {
		//배열(참조타입)
		int[] d= {80, 90, 100, 88, 68};
		int sum=0;
		double avg=sum/d.length;
		
		for(int i=0; i<=d.length-1; i++) {
			System.out.println(i+"번째 : " +d[i]);
			sum=sum+d[i];
			System.out.println();
			
		}
		avg=sum/d.length;
		System.out.println("평균 : " + avg);
	}

}
