package ex02;

public class Car {
	//필드(속성)
	private String company;
	private int maxSpeed;
	
	
	//set()메서드 
	public void setCompany(String company) {
		this.company=company;
		
		
	}
	
	//생성자(기본생성자) 찍어내는 틀이라함
	Car() {
		
	}
	
	//메서드(동작) 무슨 동작을할지 정의함
	public void printCar() {
		System.out.println("제조 회사 : " + company);
		System.out.println("최대 속도 : " + maxSpeed);
	}
}
