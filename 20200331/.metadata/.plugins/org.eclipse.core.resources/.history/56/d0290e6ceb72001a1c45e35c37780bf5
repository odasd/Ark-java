package ex01;

public class Sample {

	public static void main(String[] args) { //3가지 기본성 : 메소드 필드 클래스
		Student s1=new Student(); //기본 생성자 class에 생성자를 주지 않아도 기본으로 생성자로 된다
		//클래스   객체
		s1.sno="01"; //필드(속성)
		s1.sname="홍길동";
		s1.tel="010-5043-7887";
		s1.print(s1);
		
		Student s2=new Student();
		
		s2.sno="02";
		s2.sname="심청이";
		s2.tel="010-1234-5678";
		s2.print(s2);
		
		Student s3=new Student();
		
		s3.sno="03";
		s3.sname="김나다";
		s3.tel="010-5469-7512";
		s3.print(s3);
			
		Car car1=new Car();
		
		car1.color="검정";
		car1.model="그랜저";
		car1.maxSpeed=250;
		car1.printCar(car1);
		
		Car car2=new Car();
		
		car2.color="회색";
		car2.model="소나타";
		car2.maxSpeed=190;
		car2.printCar(car2);
		
	}

}
