package ex04;

public class Sample {

	public static void main(String[] args) {
		//배열(참조타입)
		int[] d= {90, 80, 100, 120}; //데이터를 원하는 대로 기입 가능 
		int sum=0;
		
		for(int i=0; i<=d.length-1; i++) {
			System.out.println(i+"번째 : " + d[i]);
			sum=sum+d[i];
		}
		System.out.println("합계 : " + sum);

	}

}
