package ex01;

public class Sample {

	public static void main(String[] args) {
		int score=88;
		if(score>=90) {
			System.out.println(score+"은(는)90점보다 큽니다.");
			System.out.println("등급은 A등급입니다.");
		}
		if(score<90) {
			System.out.print(score+"은(는) 90점보다 작습니다.");
			System.out.println("등급은 B입니다.");
		}
	}

}
